#!/bin/sh

alias ls="ls --color=auto"
alias grep="grep --color=auto"
alias vimgo='vim -u ~/.vimrc.go'

alias myalias="vim ~/.dotfiles/aliases"
alias myenv="vim ~/.dotfiles/zshenv"
alias myssh="vim ~/.ssh/config"

alias mysrc="cd $MY_SRC"
alias myrepo="cd $MY_REPO"
alias myproj="cd $MY_PROJ"
alias mygo="cd $MY_GO"

alias mpw="cd $MY_PWORK"
alias mpr="cd $MY_PREPO"

alias centos6="ssh centos6"
alias centos7="ssh centos7"
